<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/app_navigation_graph"
    app:startDestination="@id/featureWishNavGraph">

    <include-dynamic
        android:id="@+id/featureWishNavGraph"
        app:graphResName="feature_wish_nav_graph"
        app:moduleName="feature_wish"/>

    <include-dynamic
        android:id="@+id/featureFavouriteNavGraph"
        app:graphResName="feature_favourite_nav_graph"
        app:moduleName="feature_favourite"/>

    <include-dynamic
        android:id="@+id/featureProfileNavGraph"
        app:graphResName="feature_profile_nav_graph"
        app:moduleName="feature_profile"/>

</navigation>